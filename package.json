{
  "name": "rapo-browserify-budo-browserifycss",
  "version": "1.0.0",
  "description": "A rapid prototyping project for Browerify+Budo using Browserify-CSS to manage external CSS libs",
  "main": "entry.js",
  "scripts": {
    "start": "budo index.js --live --open -- -g [browserify-css --minify=true --output index.css]",
    "buildhtml": "html-dist --config html-dist.config.js --input index.html",
    "build": "node scripts/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterbarraud/rapo-browserify-budo-browserifycss.git"
  },
  "keywords": [
    "browserify",
    "budo",
    "prototyping",
    "browserify-css",
    "uglifyify",
    "html-minifier"
  ],
  "author": "@gapeterb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/peterbarraud/rapo-browserify-budo-browserifycss/issues"
  },
  "homepage": "https://github.com/peterbarraud/rapo-browserify-budo-browserifycss#readme",
  "devDependencies": {
    "browserify-css": "^0.12.1",
    "budo": "^10.0.4",
    "html-dist": "^0.2.0",
    "html-minifier": "^3.5.6",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "uglifycss": "0.0.27",
    "uglifyify": "^4.0.4"
  },
  "dependencies": {
    "dead-simple-grid-npm": "^1.0.0",
    "domready": "^1.0.8",
    "flexboxgrid": "^6.3.1",
    "mini.css": "^2.3.7",
    "normalize.css": "^7.0.0",
    "pui-css-buttons": "^8.3.3",
    "purecss": "^1.0.0"
  }
}
